{
  "name": "big-json-viewer",
  "version": "0.0.3",
  "description": "JavaScript Library to view big JSON structures.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "preversion": "tsc",
    "build-docs": "parcel build docs-src/index.html --out-dir docs --public-url ./",
    "watch-docs": "parcel docs-src/index.html --out-dir docs/tmp",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "karma": "karma start",
    "test": "karma start --single-run --browsers ChromeHeadless",
    "publish-patch": "npm version patch && npm publish",
    "publish-minor": "npm version minor && npm publish"
  },
  "keywords": [
    "JSON",
    "JavaScript",
    "TypeScript",
    "big",
    "JSON",
    "viewer"
  ],
  "author": "Dominik Herbst",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dhcode/big-json-viewer.git"
  },
  "devDependencies": {
    "@types/jasmine": "^2.8.6",
    "jasmine-core": "^2.99.1",
    "karma": "^2.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^1.1.1",
    "karma-typescript": "^3.0.12",
    "parcel-bundler": "^1.6.2",
    "tslint": "^5.9.1",
    "typescript": "^2.7.2"
  }
}
